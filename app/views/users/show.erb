<h2><%= @user.username %></h2>

<h3><%= @user.name %> | <%= @user.age %></h3>

<div> <%= @user.bio %> </div>
<br>

<h3><%= "#{@user.username}'s Stories:" %></h3>
<ol>
<% @user.stories.each do |us| %>
  <li><%= link_to us.title, us %></li>
  <%= link_to us.location.neighborhood, us.location %>
<% end %>
</ol>
<br>

<%= link_to "Home", stories_path %>
