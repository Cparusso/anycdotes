<h2><%= @story.title %></h2>
<% if @story.user %>
  <div>- <%= link_to @story.user.username, @story.user %></div>
<% else %>
  <div>- This user has deleted their profile.</div>
<% end %>

<h3><%= link_to @story.location.neighborhood, @story.location %></h3>

<div> <%= @story.content %> </div>
<br>

<div>Tags:</div>
<% @story.tags.each do |tag| %>
  <div> <%= link_to tag.tag_name, tag %> </div>
<% end %>
<br>
<%= link_to "Home", stories_path %>
